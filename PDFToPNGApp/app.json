{
  "expo": {
    "name": "PDF to PNG Dönüştürücü",
    "slug": "pdf-to-png-converter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "description": "PDF dosyalarınızı yüksek kalitede PNG görüntülerine dönüştürün",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "primaryColor": "#667eea",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.karimov.pdftopng",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.karimov.pdftopng",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "PDF to PNG Dönüştürücü",
      "shortName": "PDF2PNG",
      "lang": "tr",
      "scope": "/",
      "themeColor": "#667eea",
      "backgroundColor": "#667eea"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
